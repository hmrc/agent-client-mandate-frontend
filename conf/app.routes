# micro-service specific routes

GET     /assets/*file                   uk.gov.hmrc.agentclientmandate.controllers.AssetsController.at(path="/public", file)
GET     /keep-alive                     uk.gov.hmrc.agentclientmandate.controllers.ApplicationController.keepAlive

## Client URIs ##

GET     /client/email/:service          uk.gov.hmrc.agentclientmandate.controllers.client.CollectEmailController.view(service: String, mode: Option[String] ?= None)
POST    /client/email/:service          uk.gov.hmrc.agentclientmandate.controllers.client.CollectEmailController.submit(service: String, mode: Option[String] ?= None)

GET     /client/search/:service         uk.gov.hmrc.agentclientmandate.controllers.client.SearchMandateController.view(service: String)
POST    /client/search/:service         uk.gov.hmrc.agentclientmandate.controllers.client.SearchMandateController.submit(service: String)

GET     /client/review/:service         uk.gov.hmrc.agentclientmandate.controllers.client.ReviewMandateController.view(service: String)
POST    /client/review/:service             uk.gov.hmrc.agentclientmandate.controllers.client.ReviewMandateController.submit(service: String)

GET     /client/declaration/:service        uk.gov.hmrc.agentclientmandate.controllers.client.MandateDeclarationController.view(service: String)
POST    /client/declaration/:service        uk.gov.hmrc.agentclientmandate.controllers.client.MandateDeclarationController.submit(service: String)

GET     /client/confirmation/:service     uk.gov.hmrc.agentclientmandate.controllers.client.MandateConfirmationController.view(service: String)

GET     /client/remove/return-to-service           uk.gov.hmrc.agentclientmandate.controllers.client.RemoveAgentController.returnToService
GET     /client/remove/:id/:service                uk.gov.hmrc.agentclientmandate.controllers.client.RemoveAgentController.view(service: String, id: String, returnUrl: String)
POST    /client/remove/:id/:service                uk.gov.hmrc.agentclientmandate.controllers.client.RemoveAgentController.submit(service: String, id: String)
GET     /client/remove/:id/confirmation/:service   uk.gov.hmrc.agentclientmandate.controllers.client.RemoveAgentController.confirmation(service: String, id: String)

GET     /client/change/:service/:mandateId               uk.gov.hmrc.agentclientmandate.controllers.client.ChangeAgentController.view(service: String, mandateId: String)
POST    /client/change/:service/:mandateId               uk.gov.hmrc.agentclientmandate.controllers.client.ChangeAgentController.submit(service: String, mandateId: String)

