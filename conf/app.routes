# micro-service specific routes

GET     /assets/*file                   controllers.Assets.at(path="/public", file)

GET     /collect-email              uk.gov.hmrc.agentclientmandate.controllers.client.CollectEmailController.view
POST    /collect-email              uk.gov.hmrc.agentclientmandate.controllers.client.CollectEmailController.submit

GET     /search-mandate             uk.gov.hmrc.agentclientmandate.controllers.client.SearchMandateController.view
POST    /search-mandate             uk.gov.hmrc.agentclientmandate.controllers.client.SearchMandateController.submit

GET     /review-mandate             uk.gov.hmrc.agentclientmandate.controllers.client.ReviewMandateController.view

GET     /mandate-declaration        uk.gov.hmrc.agentclientmandate.controllers.client.MandateDeclarationController.view

GET     /mandate-confirmation       uk.gov.hmrc.agentclientmandate.controllers.client.MandateConfirmationController.view

GET     /agent-client-details       uk.gov.hmrc.agentclientmandate.controllers.agent.CheckClientDetailsController.checkClientDetails


GET     /select-service             uk.gov.hmrc.agentclientmandate.controllers.agent.SelectServiceController.select
POST    /select-service             uk.gov.hmrc.agentclientmandate.controllers.agent.SelectServiceController.submit

GET     /collect-agent-email/:service           uk.gov.hmrc.agentclientmandate.controllers.agent.CollectAgentEmailController.view(service: String)
